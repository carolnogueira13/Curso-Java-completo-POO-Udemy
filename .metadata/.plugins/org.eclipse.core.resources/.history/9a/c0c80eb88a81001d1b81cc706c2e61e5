import java.util.Locale;

public class Main {

	public static void main(String[] args) {
		
		int y = 32;
		double x = 10.35784;
		System.out.println(y);
		System.out.println(x);
		System.out.printf("%.2f\n", x); //printf - print formatado
		System.out.print("Olá, mundo!"); 
		System.out.println("Bom dia!"); //println print com quebra de linha
		Locale.setDefault(Locale.US); // classe para definir a localização dos EUA
		System.out.printf("%.2f\n", x);
	}

}
